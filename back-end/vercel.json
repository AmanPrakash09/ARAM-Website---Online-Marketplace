{
    "version": 2,
    "builds": [{"src": "./index.js", "use":
    "@vercel/node" }],
    "routes": [{ "src": "/auth/(google|github|facebook)/?", "dest": "/index.js" },
               { "src": "/auth/(google|github|facebook)/callback", "dest": "/index.js" },
               { "src": "/login/(success|failure)", "dest": "/index.js" },
               { "src": "/logout", "dest": "/index.js" },
               { "src": "/(.*)", "dest": "/index.js"}]
}